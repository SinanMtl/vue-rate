{
	"name": "vue-rate",
	"version": "3.1.1",
	"description": "Rate component for Vue",
	"main": "dist/vue-rate.umd.min.js",
	"types": "dist/vue-rate.umd.min.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SinanMtl/vue-rate.git"
	},
	"keywords": [
		"vue",
		"rate",
		"component",
		"vue.js",
		"vue-component",
		"vue3"
	],
	"author": "Sinan Mutlu <sinanm17@gmail.com>",
	"license": "MIT",
	"sideEffects": [
		"**/*.css"
	],
	"bugs": {
		"url": "https://github.com/SinanMtl/vue-rate/issues"
	},
	"homepage": "https://github.com/SinanMtl/vue-rate#readme",
	"scripts": {
		"dev": "vue-cli-service serve src/main.js",
		"build": "vue-cli-service build --target lib --inilne-vue --name vue-rate build.js && rm dist/demo.html && npm run types",
		"demo": "vue-cli-service build --mode=demo --dest=demo",
		"lint": "vue-cli-service lint",
		"types": "cp vue-rate.d.ts dist/vue-rate.common.d.ts && cp vue-rate.d.ts dist/vue-rate.umd.d.ts && cp vue-rate.d.ts dist/vue-rate.umd.min.d.ts "
	},
	"dependencies": {
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@vue/cli-service": "^5.0.4",
		"vue-loader": "^17.0.0",
		"vue-template-compiler": "^2.6.14"
	},
	"files": [
		"dist"
	]
}
